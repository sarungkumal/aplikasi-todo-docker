CAAA,YAAA,GAAA,EAAA;AANA;AAAA;AAAA,uBASA,KAAA,CAAA,IAAA,CAAA,OAAA,EAAA,OAAA,CATA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAtBA;AAuBA,IAAA,QAvBA,oBAuBA,EAvBA,EAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,GACA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,yBAAA,IAAA,CAAA,EAAA,IAAA,EAAA;AAAA,iBAAA,EAAA,CAAA,CADA;AAEA,gBAAA,IAAA,CAAA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AAFA;AAAA;AAAA,uBAKA,KAAA,CAAA,GAAA,CAAA,OAAA,EAAA,IAAA,CALA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAhCA;AAiCA,IAAA,UAjCA,sBAiCA,EAjCA,EAiCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,yBAAA,IAAA,CAAA,EAAA,KAAA,EAAA;AAAA,iBAAA,CAAA;AADA;AAAA;AAAA,uBAIA,KAAA,UAAA,CAAA,OAAA,EAAA;AAAA,kBAAA,IAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAAA,iBAAA,CAJA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AAzCA;AAXA,CAAA","sourcesContent":["<template>\n  <div class=\"todo pa-6\">\n    <v-text-field\n      v-model=\"newTaskTitle\"\n      @click:append=\"addTask\"\n      @keyup.enter=\"addTask\"\n      class=\"pa-3\"\n      outlined\n      label=\"Add Task\"\n      append-icon=\"mdi-plus\"\n      hide-details\n      clearable\n    ></v-text-field>\n    <v-list class=\"pt-0\" flat>\n      <div v-for=\"task in tasks\" :key=\"task.id\">\n        <v-list-item\n          @click=\"doneTask(task.id)\"\n          :class=\"{ 'blue lighten-5': task.done }\"\n        >\n          <template v-slot:default>\n            <v-list-item-action>\n              <v-checkbox :input-value=\"task.done\" color=\"primary\"></v-checkbox>\n            </v-list-item-action>\n\n            <v-list-item-content>\n              <v-list-item-title\n                :class=\"{ 'text-decoration-line-through': task.done }\"\n              >\n                {{ task.note }}\n              </v-list-item-title>\n            </v-list-item-content>\n            <v-list-item-action>\n              <v-btn @click.stop=\"deleteTask(task.id)\" icon>\n                <v-icon color=\"grey lighten-1\">mdi-trash-can</v-icon>\n              </v-btn>\n            </v-list-item-action>\n          </template>\n        </v-list-item>\n        <v-divider></v-divider>\n      </div>\n    </v-list>\n  </div>\n</template>\n\n<script>\nimport axios from \"../libs/axios\";\n\nexport default {\n  name: \"Todo\",\n  data() {\n    return {\n      newTaskTitle: \"\",\n      tasks: [],\n    };\n  },\n  async mounted() {\n    await this.fetchTasks();\n  },\n  methods: {\n    async fetchTasks() {\n      try {\n        const response = await axios.get(\"/todo\");\n        this.tasks = response.data;\n      } catch (error) {\n        console.dir(error);\n      }\n    },\n    async addTask() {\n      let newTask = {\n        note: this.newTaskTitle,\n        done: false,\n      };\n      this.tasks.push(newTask);\n      this.newTaskTitle = \"\";\n\n      try {\n        await axios.post(\"/todo\", newTask);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async doneTask(id) {\n      let task = this.tasks.filter((task) => task.id == id)[0];\n      task.done = !task.done;\n\n      try {\n        await axios.put(\"/todo\", task);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteTask(id) {\n      this.tasks = this.tasks.filter((task) => task.id !== id);\n\n      try {\n        await axios.delete(\"/todo\", { data: { id: id } });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}