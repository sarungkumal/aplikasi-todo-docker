r=\"primary\"></v-checkbox>\n            </v-list-item-action>\n\n            <v-list-item-content>\n              <v-list-item-title\n                :class=\"{ 'text-decoration-line-through': task.done }\"\n              >\n                {{ task.note }}\n              </v-list-item-title>\n            </v-list-item-content>\n            <v-list-item-action>\n              <v-btn @click.stop=\"deleteTask(task.id)\" icon>\n                <v-icon color=\"grey lighten-1\">mdi-trash-can</v-icon>\n              </v-btn>\n            </v-list-item-action>\n          </template>\n        </v-list-item>\n        <v-divider></v-divider>\n      </div>\n    </v-list>\n  </div>\n</template>\n\n<script>\nimport axios from \"../libs/axios\";\n\nexport default {\n  name: \"Todo\",\n  data() {\n    return {\n      newTaskTitle: \"\",\n      tasks: [],\n    };\n  },\n  async mounted() {\n    await this.fetchTasks();\n  },\n  methods: {\n    async fetchTasks() {\n      try {\n        const response = await axios.get(\"/todo\");\n        this.tasks = response.data;\n      } catch (error) {\n        console.dir(error);\n      }\n    },\n    async addTask() {\n      let newTask = {\n        note: this.newTaskTitle,\n        done: false,\n      };\n      this.tasks.push(newTask);\n      this.newTaskTitle = \"\";\n\n      try {\n        await axios.post(\"/todo\", newTask);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async doneTask(id) {\n      let task = this.tasks.filter((task) => task.id == id)[0];\n      task.done = !task.done;\n\n      try {\n        await axios.put(\"/todo\", task);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteTask(id) {\n      this.tasks = this.tasks.filter((task) => task.id !== id);\n\n      try {\n        await axios.delete(\"/todo\", { data: { id: id } });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n};\n</script>\n"]}]}